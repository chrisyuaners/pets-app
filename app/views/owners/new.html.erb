<h1>New Owner</h1>

<%if flash[:message] %>
  <h2><%=flash[:message]%></h2>
<%end%>

<%=form_for @owner do |f|%>
<%=f.label "first_name"%>
<%=f.text_field "first_name"%>
<%=f.label "last_name"%>
<%=f.text_field "last_name"%>
<%=f.label "age"%>
<%=f.number_field "age"%>
<%=f.label "email"%>
<%=f.text_field "email"%>
<%=f.submit "Submit"%>
<%end%>
